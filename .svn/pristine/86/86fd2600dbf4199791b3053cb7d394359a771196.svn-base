.rate {
	height: 50px;
	margin-left: -5px;
	padding: 5px;
	font-size: 25px;
	position: relative;
	cursor: pointer;
}
.rate input[type="radio"] {
	opacity: 0;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,0%);
	pointer-events: none;
}
.star-over::after {
	font-family: 'Font Awesome 5 Free';
    font-weight: 900;
	font-size: 16px;
	content: "\f005";
	display: inline-block;
	color: #FFD8A0;
	z-index: 1;
	position: absolute;
	top: 15px;
	left: 10px;
}

